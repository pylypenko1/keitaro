jQuery('#w0').authchoice();

        let postErrors = $('.post-errors');
        var selectedText;
//        bu function tanlangan textni olish uchun ishlaydi
        function getSelectedText() {
            if (window.getSelection) {
                selectedText = window.getSelection().toString();
            } else if (window.document.getSelection) {
                selectedText =window.document.getSelection().toString();
            } else if (window.document.selection) {
                selectedText = window.document.selection.createRange().text;
            }
            return selectedText;
        }
        $('.close_modal').click(function (e) {
            e.preventDefault();
            postErrors.hide().html('');
            $('.post-log-send').removeClass('btn-not');
            
            $('.before-submit').show();
            $('.after-submit').hide();
            $('.post-log-modal').removeClass('opened');
            $('#spelling-notification-comment').val('');
        });
        $('.finish-submit').click(function (e) {
            e.preventDefault();
            postErrors.hide().html('');
            $('.post-log-send').removeClass('btn-not');
            $('.before-submit').show();
            $('.after-submit').hide();
            $('.post-log-modal').removeClass('opened');
            $('#spelling-notification-comment').val('');
        });
        
        $('body').delegate('#spelling-notification-comment', 'keyup', function (event) {
                let textareaLength = $(this).val().length;
                if (selectedText.length > 0 && selectedText.length<100 && textareaLength> 0 && textareaLength < 200){
                    $('.post-log-send').removeClass('btn-not');
                    postErrors.hide().html('');
                }
                if (textareaLength > 200){
                    $('.post-log-send').addClass('btn-not');
                    postErrors.show().html(postErrors.data('long-comment-error'));
                }
            });
    
        $(document).keypress(function(event){
            var keycode = (event.keyCode ? event.keyCode : event.which);
            getSelectedText();
            let postTextLength = selectedText.length;
            if(event.originalEvent.ctrlKey && (keycode == '10' || keycode == '13') && +postTextLength>0){
                $('.post-log-modal').addClass('opened');
                    $('#spelling-notification-text').html(selectedText)
                    $('#post-log-post-text').val(selectedText)
                    $('#post-log-post-url').val(location.href)
                if(postTextLength > 100){
                    $('.post-log-send').addClass('btn-not');
                    postErrors.show().html(postErrors.data('long-text-error'));
                }
            }
        });
        $('body').delegate('.post-log-send','click', function(event){
            event.preventDefault();
            let postLogData ={};
            let postText =$('#post-log-post-text').val();
            let commentText = $(this).parents('form').find('#spelling-notification-comment').val();
          postLogData['SpellingMistake'] = {
                comment_text: commentText,
                selected_text: postText,
                current_url: location.href,
                language: siteLanguage,
            };
            if(commentText.length > 200){
                $('.post-log-send').addClass('btn-not');
                postErrors.show().html(postErrors.data('long-comment-error'));
            }
            if(commentText.length < 200 && postText.length < 100){
                $.ajax({
                    url: postLogUrl,
                    data:postLogData,
                    type: 'POST',
                    dataType: 'json',
                    success: function(response) {
                        if(response.status == 1){
                            postErrors.hide().html('');
                           $('.before-submit').hide();
                            $('.after-submit').show();
                        }else{
                            $('.post-log-modal').removeClass('opened');
                            console.log(response.message);
                        }
                        
                        $('#spelling-notification-comment').val('');
                    }
                });
            }
        });
        
});</script>        <script>
