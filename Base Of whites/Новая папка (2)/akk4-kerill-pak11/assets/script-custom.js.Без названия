		if ($(this).val() == '1'){
			$('#calculator .range_wrapper.office').removeClass('__visible');
			$('#calculator .range_wrapper.home').addClass('__visible');
			setTimeout(sliders_rahge(),500);
		}else if($(this).val() == '2'){
			$('#calculator .range_wrapper.office').addClass('__visible');
			$('#calculator .range_wrapper.home').removeClass('__visible');
			setTimeout(sliders_rahge(),500);
		}
	});
	
