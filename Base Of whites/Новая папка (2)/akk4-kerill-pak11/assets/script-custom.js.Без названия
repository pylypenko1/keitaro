	$(document).on('click','.popup_block .navigation li',function(){
		$('.popup_block .navigation li').removeClass('__active');
		$(this).addClass('__active');
		var type = $(this).attr('data-nav');

		var data = {
			'action': 'popup_trigger',
			'type': type
		};
		$.ajax({
			url:'/wp-admin/admin-ajax.php', // обработчик
			data:data, // данные
			type:'POST', // тип запроса
			success:function(data){
				if( data ) { 	
					$('#relocation_popup_form').html(data);
					} 
			}
		});


	});
