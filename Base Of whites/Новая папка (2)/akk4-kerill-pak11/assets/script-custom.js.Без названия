$('.top-panel .services').click(function (e) {
		var $this = $(this);
		var data = {
			'action': 'top_header',
		};

		var hamb = $this.find('.hamb');

		hamb.addClass('animate');

		if(hamb.hasClass('__active')){
			hamb.removeClass('animate').removeClass('__active');
			$('.js-menu').html('');
			return false;
		}



		$.ajax({
			url:'/wp-admin/admin-ajax.php', // обработчик
			data:data, // данные
			type:'POST', // тип запроса
			success:function(data){
				if( data ) {
					hamb.removeClass('animate').addClass('__active');		
					$('.js-menu').html(data); // вставляем новые посты 
				}
			}
		});
});





$('.close_field').click(function(e) {

    e.preventDefault();

    $(this).removeClass('show');
    $('.menu-trigger').removeClass('__active').find('.menu_wrapper').removeClass('__visible');

});



