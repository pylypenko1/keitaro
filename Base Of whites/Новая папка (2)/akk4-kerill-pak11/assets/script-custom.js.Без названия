	$(document).on('change','#city-form input[type="radio"]',function(){
		var city = $(this).val();
		var data = {
			action: 'city_select',
			city: city
		};

			$.ajax({
        			type: 'POST',
        			url: '/wp-admin/admin-ajax.php',
					data:data,
        			success: function(data) {
        			   location.reload();
        			},
        			error:  function(xhr, str){
        			console.log('Возникла ошибка: ' + xhr.responseCode);
        			}
        		});	
	});
	
	
	
	
	$('#clbk_popup_form,#prod_popup_form').submit(function(){
		
		var form = $(this);
		var identifier = form.find('.identifier');
		//console.log(identifier);
		form.find('.required').each(function(){
			 if($(this).val() != ''){
				  // Если поле не пустое удаляем класс-указание
					$(this).removeClass('empty_field');
					
				} else {
				  // Если поле пустое добавляем класс-указание
					$(this).addClass('empty_field');
				}
		
		});
		 
		var sizeEmpty = form.find('.empty_field').length;
	
			if(sizeEmpty != 0){
				return false;
			}
	 
		$.ajax({
			url:'/postmaster.php', // обработчик
			data:$(this).serialize(), // данные
			type:'POST', // тип запроса
			dataType: "html",
			success:function(data){
				if( data ) { 	
					$('#clbk_popup').removeClass('__visible');
					$('#clbk_scs_send_popup').addClass('__visible');
						
					if(identifier.attr('name') == 'zvonok-head'){
						yaCounter24516245.reachGoal('Zvonok');
						console.log(identifier.attr('name'));
					}
					if(identifier.attr('name') == '729'){
						yaCounter24516245.reachGoal('Zakaz bankomat');
						console.log(identifier.attr('name'));
					}
					if(identifier.attr('name') == '598'){
						yaCounter24516245.reachGoal('Zakaz seif');
						console.log(identifier.attr('name'));
					}
					if(identifier.attr('name') == '185'){
						yaCounter24516245.reachGoal('Zakaz office');
						console.log(identifier.attr('name'));
					}
					if(identifier.attr('name') == '172'){
						yaCounter24516245.reachGoal('Zakaz kvartira');
						console.log(identifier.attr('name'));
					}
					if(identifier.attr('name') == '602'){
						yaCounter24516245.reachGoal('Zakaz gruzchik');
						console.log(identifier.attr('name'));
					}
					
					if(identifier.attr('name') == '199'){
						yaCounter24516245.reachGoal('Zakaz takelag');
						console.log(identifier.attr('name'));
					}
					
					} 
			}
		});
		return false;
	});
	$('#cars_popup_form').submit(function(){
		var form = $(this);
		form.find('.required').each(function(){
			 if($(this).val() != ''){
				  // Если поле не пустое удаляем класс-указание
					$(this).removeClass('empty_field');
					
				} else {
				  // Если поле пустое добавляем класс-указание
					$(this).addClass('empty_field');
				}
		
		});
		 
		var sizeEmpty = form.find('.empty_field').length;
	
			if(sizeEmpty != 0){
				return false;
			}
		$.ajax({
			url:'/postmaster.php', // обработчик
			data:$(this).serialize(), // данные
			type:'POST', // тип запроса
			dataType: "html",
			success:function(data){
				if( data ) { 	
					$('#cars_popup').removeClass('__visible');
					$('#clbk_scs_send_popup').addClass('__visible');
					} 
			}
		});
		return false;
	});
