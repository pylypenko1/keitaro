jQuery(document).ready(function($){
	// Ленивая загрузка изображений
function loadImg() {
    var width = window.innerWidth;

    document.querySelectorAll('.lzl_img').forEach(function(el) {

        var imgSrc = el.getAttribute('data-src'),
            imgThumbSrc = el.getAttribute('data-xs-src');

        if (width <= 425 && imgThumbSrc != null) {
            el.setAttribute('src', imgThumbSrc);
        }
        else {
            el.setAttribute('src', imgSrc);
        }

    });

    document.querySelectorAll('.lzl_b_img').forEach(function(el) {

        var imgSrc = el.getAttribute('data-img-src'),
            imgThumbSrc = el.getAttribute('data-img-xs-src');

        if (width <= 425 && imgThumbSrc != null) {
            el.setAttribute('style', 'background-image: url('+ imgThumbSrc +')');
        }
        else {
            el.setAttribute('style', 'background-image: url('+ imgSrc +')');
        }

    });
}
	loadImg();

$(window).scroll(function(){
			if  ($(window).scrollTop() > 100){
				$('body').addClass("slick-on");
				if ($('#upbutton').is(':hidden')) {
	                $('#upbutton').show('slow');
	            }
				//var top_panel = $('.top-panel').height();
				//$('body').css('padding-top',top_panel);
			}else{
				$('body').removeClass("slick-on");
				$('#upbutton').hide('slow'); 
			}
		});

$(document).mouseup(function (e){ // событие клика по веб-документу
		var div = $(".popup_block .p-wrapper"); // тут указываем ID элемента
		if (!div.is(e.target) // если клик был не по нашему блоку
		    && div.has(e.target).length === 0) { // и не по его дочерним элементам
			$('#clbk_popup,#city,.popup_block').removeClass('__visible');
			$('#cars_popup').removeClass('__visible');
			$('#relocation_popup').removeClass('__visible');
			$('#clbk_scs_send_popup').removeClass('__visible');
			$('#popup_my').removeClass('__visible');
			$('body').removeClass('hidden');
			}

		var jsmenu = $(".js-menu"); // тут указываем ID элемента
		if (!jsmenu.is(e.target) // если клик был не по нашему блоку
		    && jsmenu.has(e.target).length === 0) { // и не по его дочерним элементам
			jsmenu.html("");
				if($('.services-block .hamb').hasClass('__active')){
					$('.services-block .hamb').removeClass('__active');
				}
			}
		var mainmenu = $("#menu_trigger"); // тут указываем ID элемента
		if (!$("#menu_trigger .menu_wrapper.responsive-menu").is(e.target) // если клик был не по нашему блоку
		    && $("#menu_trigger .menu_wrapper.responsive-menu").has(e.target).length === 0) { // и не по его дочерним элементам
				if($("#menu_trigger .menu_wrapper.responsive-menu").hasClass('__visible')){
					$("#menu_trigger .menu_wrapper.responsive-menu").removeClass('__visible');
					$('#menu_trigger').removeClass('__active');
				}
				
			}

		});


// Согласие на обработку данных
$('.agree').click(function(e) {

    e.preventDefault();

    $(this)
        .toggleClass('__active')
        .siblings('button')
        .toggleAttr('disabled', '');

});

