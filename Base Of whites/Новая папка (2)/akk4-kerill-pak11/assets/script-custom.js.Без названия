	event.preventDefault();

	$('.aside_nav').addClass('__visible')

});

$('.menu-trigger').click(function (e) {

    //e.preventDefault();

    $('.close_field').toggleClass('show');
    $(this).toggleClass('__active').find('.menu_wrapper').toggleClass('__visible');

});


	$('.header-mob .burger a').on('click', function (e) {
		var hamb = $(this);
		var mob_menu = $('.header-mob .menu_wrapper');
		if(hamb.hasClass('__active')){
				hamb.removeClass('__active');
				mob_menu.removeClass('__visible');
				$('body').removeClass('__hidden');
				return false;
		}
		hamb.addClass('__active');
		mob_menu.addClass('__visible'); 
		$('body').addClass('__hidden');
		$('.header-mob .menu_wrapper li a').removeClass('onHover');
		$('.header-mob .menu_wrapper li').removeClass('hover');
		$('.header-mob #menu-vypadayushhee-osnovnoe li:first-child').addClass('hover');
	});

	$('.header-mob .menu-title .close').on('click', function (e) {
		
				$('.header-mob .burger a').removeClass('__active');
				$('.header-mob .menu_wrapper').removeClass('__visible');
				$('body').removeClass('__hidden');
		
	});



		$('.header-mob .menu_wrapper li').click(function(e){
			e.preventDefault();
			var $this = $(this).find('a');
			var onHover = $this;
			var sub = $(this).find('.sub-menu');
			var linkHref = $this.attr('href');
			$('.header-mob .menu_wrapper .menu li').removeClass('hover');
			
			if (sub[0]) {
				if(linkHref && $this.hasClass('onHover')){
					location.href = linkHref;
					
					return false;
				}
				$('.header-mob .menu_wrapper .menu li a').removeClass('onHover');
				$this.addClass('onHover');
				$this.parents('li').addClass('hover');
			}else{
				location.href = linkHref;
				return false;
			}
			
		});

	$(document).mouseup(function (e) {
		var container = $("#menu");
		if (!$('.mob_menu').is(e.target) && container.has(e.target).length === 0){
			if($('.mob_menu').hasClass('active')){
				$('.mob_menu').click();
			}
			
		}
	});



$('#upbutton').click(function() {
        $('html, body').stop().animate({scrollTop : 0}, 300);
    });



function sliders_rahge(){
	var trucks_max = $('#trucks_max').val();
	var off_work_places_max = $('#off_work_places_max').val();
	var workers_max = $('#workers_max').val();
	
	$("#trucks").ionRangeSlider({
		min: 1,
		max: parseFloat(trucks_max),
		onChange: function(data) {
			$('#trucks_num').text(data['from']);
		},
		onStart: function (data) {
			$('#trucks_num').text(data['from']);
		},
	});
	
	$("#off_work_places").ionRangeSlider({
		min: 1,
		max: parseFloat(off_work_places_max),
		onChange: function(data) {
			$('#off_work_places_num').text(data['from']);
			$('#count').val(data['from']);
		},
		onStart: function (data) {
				$('#off_work_places_num').text(data['from']);
			},
	});
	$("#workers").ionRangeSlider({
		min: 1,
		max: parseFloat(workers_max),
		onChange: function(data) {
			$('#workers_num').text(data['from']);
		},
		onStart: function (data) {
				$('#workers_num').text(data['from']);
			},
	});

	$("#trucks_office").ionRangeSlider({
		min: 1,
		max: parseFloat(trucks_max),
		onChange: function(data) {
			$('#trucks_office_num').text(data['from']);
		},
		onStart: function (data) {
			$('#trucks_office_num').text(data['from']);
		},
	});
	
	$("#off_work_places_office").ionRangeSlider({
		min: 1,
		max: parseFloat(off_work_places_max),
		onChange: function(data) {
			$('#off_work_places_office_num').text(data['from']);
			$('#count').val(data['from']);
		},
		onStart: function (data) {
				$('#off_work_places_office_num').text(data['from']);
			},
	});
	$("#workers_office").ionRangeSlider({
		min: 1,
		max: parseFloat(workers_max),
		onChange: function(data) {
			$('#workers_office_num').text(data['from']);
		},
		onStart: function (data) {
				$('#workers_office_num').text(data['from']);
			},
	});
	
	
	
	

	
}
$("#furn_qnty").ionRangeSlider({
		min: 1,
		max: 1000,
		onChange: function(data) {
			$('#furn_qnty_num').text(data['from']);
		}
	});

