//On add to cart
	$(document).on('added_to_cart',function(){
		//open_popup();
		update_count(); 
		$('.woocommerce-cart-tab').show();
	});
	
	
	
	
	$(document).on('click','.woocommerce-cart-tab',function(){
		open_popup();
	});
	
	
	$(document).on('click','.add_to_cart_button.ajax_add_to_cart',function(){
		//$('body').addClass('loading');
	})
	function open_popup(){
		var data = {
			'action': 'get_cart',
		};
		$.ajax({
			url:'/wp-admin/admin-ajax.php', // обработчик
			data:data, // данные
			type:'POST', // тип запроса
			success:function(data){
				if( data ) { 	
					$('body').removeClass('loading');				
					$('head').append('<script async type="text/javascript" src="/wp-content/plugins/woocommerce/assets/js/frontend/checkout.min.js?ver=3.3.5"></script>');
					$('#custom-cart').html(data); // вставляем новые посты 
					$('.custom-cart-wr').show();
					$('.site-overflow').show();
					$('#custom-cart .quantity .qty').styler();
					
					update_count();
					//$('.payment_method_cheque').hide();
					//$("#payment_method_cod").prop("checked", true);
					//$('#payment-custom-field').hide();
					
					 
					// $('label[for="payment_method_cheque"]').append('<span id="upload_order">Прикрепить файл с реквизитами</span><input id="tstFile" type="file" name="file-upload" />');
					//$('label[for="shipping_method_0_local_pickup1"]').after('<span class="caption">Самовывоз со склада <span>Москва, ул. Кольская, д.7</span></span>');
					//$('label[for="shipping_method_0_flat_rate2"]').after('<span class="caption">Доставка в течение 1 дня <span>400 руб.</span></span>')
					//$('.payment_method_cheque').after('<a class="link_1" style="display: none;float: right;width: 30%;">Ввести банковские реквизиты вручную</a><a class="link_2" style="display: none;float: right;width: 30%;">Загрузить файл</a>')
				} 
			}
		});
	}
	function update_count(){
		var data = {
			'action': 'get_cart_count',
		};
		$.ajax({
			url:'/wp-admin/admin-ajax.php', // обработчик
			data:data, // данные
			type:'POST', // тип запроса
			success:function(data){
				if( data ) { 	
						$('.woocommerce-cart-tab .woocommerce-cart-tab__contents').html(data);
					} 
			}
		});
	}
	$(document).on('click','.site-overflow,#custom-cart .close',function(){
		$('.site-overflow').css('display','none');
		$('.custom-cart-wr').hide();
	})
	
	
	
