$('.thumb_imgs .item').click(function(e) {

	e.preventDefault();

	var $imgSrc = $(this).data('main-img');

	$('.main_img').attr('src', $imgSrc);

	$('.thumb_imgs .item').removeClass('__active');
	$(this).addClass('__active');

});



$('.tabs_header a').click(function(e) {

	e.preventDefault();

	$(this).addClass('__active').siblings('a').removeClass('__active');

	var $wrapperId = $(this).data('tab-id');

	console.log('.tabs .content #item_' + $wrapperId);

	$('.tabs .content #item_' + $wrapperId).addClass('__visible').siblings('.item').removeClass('__visible');

});


	
