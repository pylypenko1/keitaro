		$("#payment_method_cheque").prop("checked", true);
		$('#payment-custom-field .form-row').removeClass('validate-required');
		$('#payment-custom-field .form-row .input-text').removeClass('empty_field');
	});
	$(document).on('click','.link_1',function(){
		$('#payment-custom-field').slideDown('slow');
		$('.link_2').show();
		$(this).hide();
		$('#payment-custom-field .form-row').addClass('validate-required');
	});
	$(document).on('click','.link_2',function(){
		$('#payment-custom-field').slideUp('slow');
		$('.link_1').show();
		$(this).hide();
		$('#payment-custom-field .form-row').removeClass('validate-required');
		$('#payment-custom-field .form-row').removeClass('empty_field');
	});
	/*$(document).on('click','.custom-cart-wr',function(){
		$('.custom-cart-wr').hide();

	});*/
	
	
	$(document).on('change','.variations_button .qty', {} ,function(e){
		e.preventDefault();
          var  inputQty = $(this);
			 var inputQty_name = inputQty.attr('prod_id');
			qty_update(inputQty.val(),inputQty_name);
			$('#prod_add').attr('qty',inputQty.val());
			
            return false;
        });





	function qty_update(qty,key){
		var data = {
		   'action': 'update_price',
		   'prod_id':key,
		   'qty':qty
		 };
		 $.post(
		   '/wp-admin/admin-ajax.php', // The AJAX URL
		   data, // Send our PHP function
		   function(response){

			$('.variations_button .total .price').html(response);
			
			$('#price_help').val(response);
			
		   }
		 );
	}
	
	$(document).on('change','input.arenda_qty',function(e){
          var test = $(this).val();
          $('#prod_add').attr('arenda_qty',test);
          $('.variations_button .total .price').html(parseFloat($('#price_help').val())*test);
        });

});