		$('#city').addClass('__visible');
		$('body').addClass('hidden');
	});


	$(document).on('click','.js-relocation',function(){
		$('#relocation_popup').addClass('__visible');
		$('body').addClass('hidden');
		var identifier = $(this).attr('data-identifier');
		if($('.identifier')){
			$('.identifier').remove();
		}
		$('#relocation_popup form').append('<input type="hidden" class="identifier" name="'+identifier+'" />');
		$('.select').remove();
	});
	$(document).on('click','.close_popup_field, .close',function(){
		$('#clbk_popup,#city,#prod_popup').removeClass('__visible');
		$('#cars_popup').removeClass('__visible');
		$('#relocation_popup').removeClass('__visible');
		$('#clbk_scs_send_popup').removeClass('__visible');
		$('#popup_my').removeClass('__visible');
		$('#clbk_popup').find('.h3').html('Заказать<br>звонок');
		$('#clbk_popup').find('input[name="action"]').val('call');
		$('body').removeClass('hidden');
	});
