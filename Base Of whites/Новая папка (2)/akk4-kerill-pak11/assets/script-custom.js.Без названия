        $('.quantity').on('click', '.plus', function(e) {
            var input = $(this).parent().find('.num');
            input.val(parseInt(input.val())+1).change();
			//$(this).parent().parent().find('a.ajax_add_to_cart').attr('data-quantity',val+1)
        });
		$('.quantity').on('change', '.num', function(e) {
            $input = $(this).val();
            var val = parseInt($input);
            //$input.html( val+1 ).change();
			$(this).parent().parent().find('a.ajax_add_to_cart').attr('data-quantity',val)
        });
        $('.quantity').on('click', '.minus', function(e) {
            $input = $(this).parent().find('.num');
            var val = parseInt($input.val());
			
            if (val > 1) {
                $input.val( val-1 ).change();
				$(this).parent().parent().find('a.ajax_add_to_cart').attr('data-quantity',val-1)
            } 
        });
   
   
   $(document).on('change','#custom-cart .quantity .qty', {} ,function(e){
		e.preventDefault();
          var  inputQty = $(this);
			 var inputQty_name = inputQty.attr('name');
			qty_update_1(inputQty.val(),inputQty_name);
            return false;
        });

	function qty_update_1(qty,key){
		var data = {
		   'action': 'mode_theme_update_mini_cart',
		   'key_i':key,
		   'input_val':qty
		 };
		 $.post(
		   '/wp-admin/admin-ajax.php', // The AJAX URL
		   data, // Send our PHP function
		   function(response){
			$('#custom-cart .cart-basket-body .woocommerce .js-cart-response').html(response); // Repopulate the specific element with the new content
			$('#custom-cart .quantity .qty').styler();
		   }
		 );
	}
   
   $('#custom-cart .quantity .qty ').styler();
  // $('#pa_kolichestvo-sutok').styler();
   $('input[type="number"]').styler();
   
$(document).on('change','#pa_kolichestvo-sutok',function(){
	//$('#pa_kolichestvo-sutok').change(); 
});
   
   
	$( document ).on('click','#custom-cart .remove',function(event) {
	event.preventDefault();
	var key = $(this).attr('data-key');
	var data = {
	   'action': 'mode_theme_update_mini_cart',
	   'key':key
	 };
	 $.post(
	   '/wp-admin/admin-ajax.php', // The AJAX URL
	   data, // Send our PHP function
	   function(response){
		 $('#custom-cart .cart-basket-body .woocommerce .js-cart-response').html(response); // Repopulate the specific element with the new content
	   $('#custom-cart .quantity .qty').styler();
	   }
	 );
	
	});
	
	$(document).on('change','#tstFile',function(){
	  var file = $(this).val();
	  var v = file.search(/^.*\.(?:jpg|jpeg|pdf|docx|png|doc|xls|xlsb|xlsx|txt|rtf|gif|xml|mobi|ods|pps)\s*$/ig)
	  if(v!=0){
		 alert("Неправильный формат файла\n Форма будет очищена");
		 $('#tstFile').val('');
		  $('#upload_order').html('Прикрепить файл с реквизитами');
		}
		 $('#upload_order').html(file.replace(/C:\\fakepath\\/i, ''));
	})
	$(document).on('submit','form.checkout_my',function(e){	
		var form = $(this);
		
			
		form.find('.validate-required').each(function(){
			 if($(this).find('.input-text').val() != ''){
				  // Если поле не пустое удаляем класс-указание
					$(this).removeClass('empty_field');
					
				} else {
				  // Если поле пустое добавляем класс-указание
					$(this).addClass('empty_field');
				}
		
		});
		 
		var sizeEmpty = form.find('.empty_field').length;
	
			if(sizeEmpty == 0){
				form.submit();
				 return true;
			}else{
				return false;
			}
	  e.preventDefault(); 
    return false;
	});
	
	
	$(document).on('change','#billing_new_fild11',function(){
