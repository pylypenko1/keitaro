	$('.ajax-calc-form').submit(function(){
		var form = $(this);
		var identif = form.find('input[name="services_items"]').val();
		console.log(identif);
		form.find('.required').each(function(){
			 if($(this).val() != ''){
				  // Если поле не пустое удаляем класс-указание
					$(this).removeClass('empty_field');
					
				} else {
				  // Если поле пустое добавляем класс-указание
					$(this).addClass('empty_field');
				}
		
		});
		 
		var sizeEmpty = form.find('.empty_field').length;
	
			if(sizeEmpty != 0){
				return false;
			}
		
		$.ajax({
			url:'/postmaster.php', // обработчик
			data:$(this).serialize(), // данные
			type:'POST', // тип запроса
			dataType: "html",
			success:function(data){
				if( data ) { 	
					$('#clbk_scs_send_popup').addClass('__visible');
					if(identif == "Квартирный переезд"){
						yaCounter24516245.reachGoal('Zakaz calc kvartira');
						console.log('Квартирный переезд');
					}
					if(identif == "Офисный переезд"){
						yaCounter24516245.reachGoal('Zakaz calc ofis');
						console.log('Офисный переезд');
					}
					return true;
					} 
			}
		});
		return false;
	});
	
	
	$(document).on('click','#upload_order',function(){
