		$('#modal-privacy').addClass('__visible');
		$('body').addClass('hidden');
	});
	$(document).on('click','.js-call,.one_click',function(){
		$('#clbk_popup').addClass('__visible');
		if($(this).hasClass('one_click')){
			$('#clbk_popup').find('.h3').html('Быстрый<br>заказ');
		}
		$('body').addClass('hidden');
		var identifier = $(this).attr('data-identifier');
		if($('.identifier')){
			$('.identifier').remove();
		}
		$('#clbk_popup_form').append('<input type="hidden" class="identifier" name="'+identifier+'" />');
		
	});
$(document).on('click','.product_click',function(){
		$('#prod_popup').addClass('__visible');
		$('body').addClass('hidden');
		
		var product_name = $(this).attr('product_name');
		var sutki = $(this).attr('qty');
		var qty = $(this).attr('arenda_qty');

		$('#prod_popup').find('input[name="product_name"]').val(product_name);
		$('#prod_popup').find('input[name="sutki"]').val(sutki);
		$('#prod_popup').find('input[name="qty"]').val(qty);
	});
